



# Ollama Configuration
OLLAMA_HOST=http://ollama:11434
OLLAMA_MODEL=qwen2.5-coder:1.5b

# Timeouts (secondes)
OLLAMA_TIMEOUT=90
TOOL_TIMEOUT=30
EXECUTION_TIMEOUT=5

# Limites
MAX_CODE_LENGTH=10000
MAX_TOKENS=3000
MAX_TOOL_ITERATIONS=5

# Cache
CACHE_ENABLED=true
CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO

# Features
CODE_EXECUTION_ENABLED=true
WEB_SEARCH_ENABLED=true
SANDBOX_ENABLED=true

